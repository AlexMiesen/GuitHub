<%= simple_form_for(@instrument) do |f| %>
<%= f.input :name %>
<%= f.input :category, collection: ['guitars', 'keyboards', 'brass', 'percussion', 'orchestral'] %>
<%= f.input :description %>
<%= f.input :location %>
<%= f.input :price_per_day %>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>
<div class="btn-forms">
  <%= f.submit 'Save', class: "btn-save btn btn-warning" %>
</div>
<% end %>
